{
    "name": "nginx_watchdog",
    "universal_id": "nginx-sentinel",
    "constraints": [],
    "config": {
        "ui": {
            "agent_tree": {
                "emoji": "\ud83c\udf89"
            }
        },
        "check_interval_sec": 10,
        "always_alert": 1,
        "restart_limit": 3,
